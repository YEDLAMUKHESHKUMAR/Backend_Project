<% if(successMsg && successMsg.length){ %>
    <!-- <div class="alert alert-success alert-dismissible fade show col-6 offset-3" role="alert"> -->
    <div class="alert alert-success alert-dismissible fade show col-lg-6 col-md-10 offset-md-1 offset-lg-3" role="alert">
        <%= successMsg  %>
        <button class="btn-close" type="button" data-bs-dismiss="alert" aria-label></button>
    </div>
<% } %>
<% if(errorMsg && errorMsg.length){ %>
    <!-- <div class="alert alert-danger alert-dismissible fade show col-6 offset-3" role="alert"> -->
    <div class="alert alert-success alert-dismissible fade show col-lg-6 col-md-10 offset-md-1 offset-lg-3" role="alert">
        <%= errorMsg  %>
        <button class="btn-close" type="button" data-bs-dismiss="alert" aria-label></button>
    </div>
<% } %>